	  js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=333859243294743";
	  fjs.parentNode.insertBefore(js, fjs);
	}(document, 'script', 'facebook-jssdk'));</script>
<%= yield %>

</body>
